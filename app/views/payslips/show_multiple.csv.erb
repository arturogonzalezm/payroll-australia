name,pay_period,gross_income,income_tax,net_income,super
<%= CSV.generate do |csv|
  @payslips.each do |ps|
    csv << [ps.name,
            ps.pay_period,
            ps.gross_income,
            ps.income_tax,
            ps.net_income,
            ps.super_amount
            ]
  end
end.html_safe %>
